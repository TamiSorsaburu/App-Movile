<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ConfirmarTurno">

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="244dp"
        android:layout_height="236dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="315dp"
        android:background="@drawable/estilo_bordes"
        android:backgroundTint="#F6BB46"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/confirmar_turno5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="162dp"
        android:layout_marginTop="24dp"
        android:text="@string/confirmar_turno5"
        android:textColor="@color/secundario"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/confirmar_turno3"
        app:layout_constraintEnd_toEndOf="@+id/confirmar_turno3"
        app:layout_constraintStart_toStartOf="@+id/confirmar_turno3"
        app:layout_constraintTop_toTopOf="@+id/confirmar_turno3" />

    <TextView
        android:id="@+id/confirmar_turno1"
        android:layout_width="345dp"
        android:layout_height="97dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="660dp"
        android:text="@string/confirmar_turno1"
        android:textColor="@color/negro"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/confirmar_turno2"
        android:layout_width="353dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:text="@string/confirmar_turno2"
        android:textColor="@color/negro"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8" />

    <TextView
        android:id="@+id/confirmar_turno3"
        android:layout_width="362dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="8dp"
        android:text="@string/confirmar_turno3"
        android:textColor="@color/negro"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/confirmar_turno2" />

    <Button
        android:id="@+id/btn_atras_turno"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/background_dorado"
        android:onClick="botonCancelar"
        android:text="@string/btn_atras_turno"
        android:textColor="@color/blanco"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/confirmar_turno3" />

    <TextView
        android:id="@+id/onfirmar_turno4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="20dp"
        android:text="@string/confirmar_turno4"
        android:textColor="@color/secundario"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/confirmar_turno2"
        app:layout_constraintEnd_toEndOf="@+id/confirmar_turno2"
        app:layout_constraintStart_toStartOf="@+id/confirmar_turno2"
        app:layout_constraintTop_toTopOf="@+id/confirmar_turno2" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="204dp"
        android:layout_height="210dp"
        android:layout_marginStart="94dp"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="94dp"
        android:layout_marginBottom="319dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/checkmark" />

</androidx.constraintlayout.widget.ConstraintLayout>